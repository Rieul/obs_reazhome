@article{2020_labussiere.nadai,
  title = {Les temporalités entremêlées des processus de transition énergétique},
  author = {Labussière, Olivier and Nadaï, Alain},
  date = {2020-12-21},
  journaltitle = {Socio-anthropologie},
  number = {42},
  pages = {93--106},
