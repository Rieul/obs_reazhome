@article{2019_bradshaw.denardis,
  title = {Privacy by {{Infrastructure}}: {{The Unresolved Case}} of the {{Domain Name System}}},
  shorttitle = {Privacy by {{Infrastructure}}},
  author = {Bradshaw, Samantha and DeNardis, Laura},
  date = {2019},
  journaltitle = {Policy \& Internet},
  volume = {11},
  number = {1},
  pages = {16--36},
  issn = {1944-2866},
  doi = {10.1002/poi3.195},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/poi3.195},
  urldate = {2019-03-29},
  abstract = {Digital privacy concerns are primarily viewed through the lens of personal data and content. But beneath the layer of content, less visible issues of infrastructure design and administration raise significant privacy concerns. The Internet's Domain Name System (DNS) is one such terrain. There is already a great deal of attention around how the DNS intersects with freedom of speech, trademark disputes, cybersecurity challenges, and geopolitical power struggles in the aftermath of transitioning the historic U.S. oversight role to the global multistakeholder Internet governance community. However, the privacy implications embedded in the technical architecture of the DNS have received less attention, perhaps because these issues are concealed within complex technical arrangements outside of public view. This article explores privacy issues in the DNS by examining two contemporary, and still unresolved, case studies: the WHOIS system as a de facto Internet identity system revealing website registrants; and privacy in domain name queries, which have historically been unencrypted and therefore reveal personal information about what sites individuals visit. DNS privacy challenges not only demonstrate the important connection between infrastructure and rights, but also exemplify how cross-border, universal technologies come into conflict with the bounded laws of nation states. It is a critical moment of opportunity to examine these cases because their resolution will help determine the future of basic privacy rights online.},
  langid = {english},
  keywords = {búsquedas de DNS,ciberseguridad,cybersecurity,DNS queries,DNS 请求,Domain Name System,freedom of expression,gobernanza de Internet,Internet governance,libertad de expresión,privacidad,privacy,Sistema de Nombres de Dominio,WHOIS,域名系统,网络安全,网络治理,言论自由,隐私}
